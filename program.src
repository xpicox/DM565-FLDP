function f_iteratively( n ){
    var i, res
    i = 1;
    res = 0;
    while i <= n do {
        res = res + i;
        i = i + 1;
    }
    return res;
}
     
function f_recursively( n ){
    if n <= 1 then
        return 1;
    else
        return n + f_recursively( n - 1 );
}

print f_iteratively( 5 );
print f_recursively( 5 );
